    "@backstage/plugin-permission-node": "workspace:^",
    "@backstage/plugin-proxy-backend": "workspace:^",
    "@backstage/plugin-scaffolder-backend": "workspace:^",
    "@backstage/plugin-scaffolder-backend-module-github": "workspace:^",
