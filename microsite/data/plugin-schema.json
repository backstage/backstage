{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$ref": "#/definitions/plugin",
  "definitions": {
    "plugin": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "title": {
          "type": "string"
        },
        "author": {
          "type": "string"
        },
        "authorUrl": {
          "type": "string",
          "format": "uri"
        },
        "category": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "documentation": {
          "type": "string",
          "format": "uri"
        },
        "iconUrl": {
          "type": "string"
        },
        "npmPackageName": {
          "type": "string"
        },
        "addedDate": {
          "type": "string",
          "format": "date"
        },
        "order": {
          "type": "number"
        }
      },
      "required": [
        "addedDate",
        "author",
        "authorUrl",
        "category",
        "description",
        "documentation",
        "npmPackageName",
        "title"
      ],
      "title": "plugin"
    }
  }
}
