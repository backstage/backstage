{
    "name": "Backstage Dev Container",
    "dockerFile": "Dockerfile",
    "context": "..",
    "forwardPorts": [
        3000,
        7007
    ],
    "postCreateCommand": "yarn install && yarn dev",
    "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
    },
    "extensions": [
        "esbenp.prettier-vscode",
        "dbaeumer.vscode-eslint"
    ],
    "remoteUser": "node"
}